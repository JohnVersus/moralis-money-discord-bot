// schema.prisma

generator client {
  provider = "prisma-client-js"
}

// datasource db {
//   provider = "postgresql"
//   url      = env("DATABASE_URL")
// }

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model UserVerification {
  id        BigInt   @id @default(autoincrement())
  discordId String   @unique
  mmId  String   @unique
  createdAt DateTime @default(now())
}
